{
  "name": "babel-plugin-react-icons",
  "version": "0.0.1",
  "description": "Modular react-icons builds without the hassle.",
  "main": "dist/index.js",
  "author": "Ward Peeters <ward@coding-tech.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/helper-module-imports": "^7.12.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/helper-plugin-test-runner": "^7.10.4",
    "@babel/plugin-syntax-jsx": "^7.12.1",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.1",
    "microbundle": "^0.12.4",
    "prettier": "^2.1.2"
  },
  "scripts": {
    "build": "microbundle -f cjs --target=node --sourcemap=false",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,html,css}": "prettier --write --list-different --ignore-unknown",
    "*.js": [
      "eslint --fix"
    ]
  }
